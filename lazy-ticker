$(function(n){n.fn.lazyticker=function(){return this.each(function(){var n,t=0,c=$(".breaking-ticker .breaking-post");function e(){null!=n&&(clearInterval(n),n=null)}function i(){n=setInterval(function(){a(1)},4e3)}function a(n){var e,i,a;c.not(t).removeClass("active"),e=t,i=n,a=c.length,t=(e+i+a)%a,c.eq(t).addClass("active")}jQuery(function(){c.first().addClass("active"),i(),$(".next").click(function(n){e(),a(1),i()}),$(".prev").click(function(n){e(),a(-1),i()})})})}});
